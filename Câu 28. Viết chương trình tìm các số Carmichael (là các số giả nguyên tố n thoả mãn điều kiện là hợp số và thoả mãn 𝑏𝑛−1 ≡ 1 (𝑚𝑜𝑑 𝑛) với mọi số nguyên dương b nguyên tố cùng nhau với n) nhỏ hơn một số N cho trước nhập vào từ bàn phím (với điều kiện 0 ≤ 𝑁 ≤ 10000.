tá»‘ cÃ¹ng nhau vá»›i n) nhá» hÆ¡n má»™t sá»‘ N cho trÆ°á»›c nháº­p vÃ o tá»« bÃ n phÃ­m (vá»›i Ä‘iá»u kiá»‡n 0 â‰¤
ğ‘ â‰¤ 10000.


#include <stdio.h>
#include <stdbool.h>

// HÃ m kiá»ƒm tra tÃ­nh nguyÃªn tá»‘
bool is_prime(int number) {
    if (number < 2) return false;
    for (int i = 2; i * i <= number; i++) {
        if (number % i == 0) return false;
    }
    return true;
}

// HÃ m tÃ­nh lÅ©y thá»«a theo mÃ´ Ä‘un
int modular_exponentiation(int base, int exp, int mod) {
    int result = 1;
    base = base % mod;
    while (exp > 0) {
        if (exp % 2 == 1)
            result = (result * base) % mod;
        exp = exp >> 1;
        base = (base * base) % mod;
    }
    return result;
}

// HÃ m tÃ¬m UCLN (GCD) cá»§a hai sá»‘
int gcd(int a, int b) {
    while (b != 0) {
        int t = b;
        b = a % b;
        a = t;
    }
    return a;
}

// HÃ m kiá»ƒm tra tÃ­nh Carmichael cá»§a má»™t sá»‘
bool is_carmichael(int n) {
    if (is_prime(n)) return false; // n pháº£i lÃ  há»£p sá»‘
    for (int b = 2; b < n; b++) {
        if (gcd(b, n) == 1) { // b vÃ  n pháº£i nguyÃªn tá»‘ cÃ¹ng nhau
            if (modular_exponentiation(b, n - 1, n) != 1)
                return false;
        }
    }
    return true;
}


int main() {
    int N;
    printf("Nhap N (0 <= N <= 10000): ");
    scanf("%d", &N);

    if (N < 0 || N > 10000) {
        printf("Gia tri N khong hop le!\n");
        return 1;
    }

    printf("Cac so Carmichael nho hon %d la:\n", N);
    for (int n = 2; n < N; n++) {
        if (is_carmichael(n)) {
            printf("%d\n", n);
        }
    }

    return 0;
}
